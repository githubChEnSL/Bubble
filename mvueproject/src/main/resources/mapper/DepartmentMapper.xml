<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.mvueproject.mvueproject.mapper.DepartmentMapper">
    <!--  通用查询映射结果  -->
    <resultMap id="BaseResultMap" type="com.mvueproject.mvueproject.pojo.Department">
        <id property="departId" column="depart_id"/>
        <result property="departName" column="depart_name"/>
    </resultMap>
    <select id="departmentList" resultMap="BaseResultMap">
        select depart_id,depart_name
        from department
    </select>
</mapper>